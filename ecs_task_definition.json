{
  "containerDefinitions": [
    {
      "name": "app1",
      "image": "docker.io/$DOCKERHUB_USER/testdevsecopsjunto:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "$AWS_ACCESS_KEY_ID",
          "value": "$AWS_ACCESS_KEY_ID"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "$AWS_SECRET_ACCESS_KEY"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/testedevsecops",
          "awslogs-region": "$AWS_DEFAULT_REGION",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "testedevsecops",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ]
}